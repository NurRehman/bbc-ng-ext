{
  "manifest_version": 2,
  "name"            : "Search Movies from IMBb",
  "description"     : "Search Movies, TV Shows, series or episode directly from IMBb",
  "short_name"      : "Search Movies",
  "version"         : "0.1",
  "browser_action": {
    "default_popup": "index.html",
    "default_icon" : "images/logo.png"
  },
  "permissions": [
    "https://www.omdbapi.com/",
    "debugger"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://ssl.google-analytics.com; object-src 'self'"
}
